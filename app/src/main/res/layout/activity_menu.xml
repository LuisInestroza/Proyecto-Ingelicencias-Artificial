<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/house"
        tools:context=".Menu">

    <Button
            android:text="@string/btnAgregarInmueble"
            android:layout_width="159dp"
            android:layout_height="114dp"
            android:id="@+id/btnAgregarInmueble"
            android:drawableTop="@drawable/baseline_add_box_black_48" tools:text="Agregar" android:textStyle="bold"
            android:textAllCaps="true"
            android:textSize="18sp"
            android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginRight="8dp"
            android:layout_marginStart="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="8dp" android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.906" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="8dp" app:layout_constraintVertical_bias="0.268"/>
    <Button
            android:text="@string/btnListarInmueble"
            android:layout_width="163dp"
            android:layout_height="115dp"
            android:id="@+id/btnListarInmueble"
            android:drawableTop="@drawable/baseline_domain_black_48" tools:text="Ver Inmuebles"
            android:textStyle="bold" android:textSize="18sp"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginRight="8dp" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.08" app:layout_constraintVertical_bias="0.267"/>
    <Button
            android:text="@string/btnModificar"
            android:layout_width="166dp"
            android:layout_height="105dp"
            android:id="@+id/btnModificar"
            android:drawableTop="@drawable/baseline_settings_applications_black_48" tools:text="Configurar"
            android:textStyle="bold" android:textSize="18sp"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginRight="8dp"
            app:layout_constraintHorizontal_bias="0.082" app:layout_constraintVertical_bias="0.565"/>
    <Button
            android:text="@string/btnUsuarios"
            android:layout_width="156dp"
            android:layout_height="110dp"
            android:id="@+id/btnUsuarios"
            android:drawableTop="@drawable/baseline_person_add_black_48" tools:text="Usuarios"
            android:textStyle="bold" android:textSize="18sp"
            android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginRight="8dp" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.907"
            app:layout_constraintVertical_bias="0.562"/>
</android.support.constraint.ConstraintLayout>